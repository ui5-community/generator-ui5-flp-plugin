{
  "_version": "1.5.0",
  "sap.app": {
    "_version": "1.3.0",
    "id": "<%=appId%>",
    "type": "component",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{pluginTitle}}",
    "description": "{{pluginDescription}}",
    "crossNavigation": {
      "inbounds": {
        "Shell-plugin": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "Shell",
          "action": "plugin",
          "hideLauncher": true
        }
      }
    }
  },
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": []
  },
  "sap.ui5": {
    "_version": "1.2.0",
    "contentDensities": {
      "compact": true,
      "cozy": false
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "<%=appId%>.i18n.i18n"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.38.1",
      "libs": {
        "sap.ui.core": {
          "minVersion": "1.38.1"
        },
        "sap.m": {
          "minVersion": "1.38.1"
        }
      }
    },
    "componentName": "<%=appId%>"
  },
  "sap.flp": {
    "type": "plugin",
    "config": {}
  }
}
